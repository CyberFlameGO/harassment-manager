{
  "name": "harassment-manager",
  "version": "0.0.0",
  "scripts": {
    "build:all:dev": "npm run build:app:dev && npm run build:server",
    "build:all:prod": "npm run build:app:prod && npm run build:server",
    "build:app:dev": "ng build",
    "build:app:prod": "ng build --configuration production",
    "build:server": "tsc -p src/tsconfig.server.json",
    "check-format": "gts check",
    "develop": "concurrently --names 'tsc,server,app' --prefix-colors blue,green,red \"npm run watch:tsc\" \"npm run watch:server\" \"npm run watch:app\"",
    "e2e": "ng e2e",
    "fix-format": "gts fix",
    "lint": "ng lint",
    "ng": "ng",
    "setup-server": "mkdir -p build/server/ && mkdir -p build/config/ && rsync --ignore-existing src/server/server_config.template.json build/config/server_config.json",
    "start": "npm run start:prod-server",
    "start:dev-server": "NODE_ENV='development' node --max_old_space_size=4096 build/server/run_server.js build/config/server_config.json",
    "start:prod-server": "NODE_ENV='production' node --max_old_space_size=4096 build/server/run_server.js build/config/server_config.json",
    "test:server": "jasmine build/server/serving_test.js",
    "test": "NODE_OPTIONS=--max_old_space_size=4096 ng test && npm run test:server",
    "watch:app": "ng serve --port 8080",
    "watch:server": "NODE_ENV='development' nodemon --max_old_space_size=4096 --inspect build/server/run_server.js build/config/server_config.json",
    "watch:tsc": "tsc -w -p src/tsconfig.server.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.1.3",
    "@angular/cdk": "^13.1.3",
    "@angular/common": "^13.1.3",
